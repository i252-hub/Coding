//Create a student object. The student object will have first name,
// last name, age, skills, country, enrolled keys and values for the keys.
//  Store the student object in your browser localStorage.

const student = {
    firstName: 'Victorique',
    lastName: 'Bloisse',
    age: 21,
    skills: ['Javascript', 'Php', 'Java'],
    country: 'Philippines',
    status: 'enrolled'
}

const user = JSON.stringify(student, undefined, 4)
localStorage.setItem('student', user)
console.log(localStorage)
